#include<stdio.h>

int main()
    {double meal,tip,tax,total;
     scanf("%lf",&meal);
     scanf("%lf",&tip);
     scanf("%lf",&tax);
     
     total=(meal+meal*(tip/100+tax/100));
     if(total>(int)total+0.5)
         total+=1;
    
    printf("The total meal cost is %d dollars.",(int)total);
    return 0;
    }
