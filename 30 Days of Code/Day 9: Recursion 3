#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

double factorial(int n)
    {if(n==1)
    {return 1;}
    else
    return factorial(n-1)*n;
    }

int main() {int n;
            
scanf("%d",&n);
    
    printf("%d",(int)factorial(n));
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    return 0;
}

